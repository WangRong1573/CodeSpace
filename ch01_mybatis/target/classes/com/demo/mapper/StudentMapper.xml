<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="testSelect">
    <select id="getOneById" resultType="com.demo.domain.Student" parameterType="java.lang.Integer">
    select * from t_student where id = #{id}
  </select>

    <!--如果返回的是多条记录，resultType返回类型应该为集合的泛型-->
    <select id="getAll" resultType="com.demo.domain.Student">
        select * from t_student
    </select>
</mapper>