<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.ems.dao.UserDao">
    <insert id="add">
        insert into t_user(username,realname,password,sex,status,registerTime)
         values (#{username},#{realname},#{password},#{sex},#{status},#{registerTime})
    </insert>
</mapper>